{
  "deployEnv": "depeast2",
  "project": "depv2",
  "region": "us-east-2",
  "bucketRegion": "us-east-1",

  "configData": {
    "databaseName": "fsa-fpac-db",
    "dynamoTableName": "FsaFpacMetadata"
  },

  "ssm": {
    "landingBucketNameParam": "fpacFsaLandingBucketSSMName",
    "cleanBucketNameParam": "fpacFsaCleanBucketSSMName",
    "finalBucketNameParam": "fpacFsaFinalBucketSSMName",
    "glueJobRoleArnParam": "fpacFsaGlueJobRoleSSMArn",
    "etlRoleArnParam": "fpacFsaLambdaExecuteRoleARN",
    "thirdPartyLayerArnParam": "fpacfsaThirdPartyLayerArn",
    "customLayerArnParam": "fpacfsaCustomLayerArn"
  },
  "strparams": {
    "landingBucketNameParam": "punkdev-fpacfsa-landing-zone",
    "cleanBucketNameParam": "punkdev-fpacfsa-cleansed-zone",
    "finalBucketNameParam": "punkdev-fpacfsa-final-zone",
    "glueJobRoleArnParam": "arn:aws:iam::335965711887:role/FPACGlueCDKRole",
    "etlRoleArnParam": "arn:aws:iam::335965711887:role/FPACLambdaCDKRole",
    "thirdPartyLayerArnParam": "arn:aws:lambda:us-east-2:335965711887:layer:fpac-thirdparty:1",
    "customLayerArnParam": "arn:aws:lambda:us-east-2:335965711887:layer:fpac-custom:1"
  },
  "paths": {
    "glueRootPath": "../src/glue/",
    "lambdaRootPath": "../src/lambda/"
  },

  "artifacts": {
    "artifactBucket": "dev-cars-artifacts",
    "prefix": "depv2/"
  },

  "stepFunctions": {
    "createRoleIfMissing": false,
    "roleName": "FSA-dev-FPACCARS-SfnExec",
    "roleArn": "arn:aws:iam::335965711887:role/FSA-dev-FPACCARS-SfnExec"
  }
}
