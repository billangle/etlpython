{
  "deployEnv": "steam-dev",
  "project": "FMMI",
  "region": "us-east-1",
  "bucketRegion": "us-east-1",
  "SecretId": "FSA-CERT-Secrets",

  "configData": {
    "databaseName": "fsa-fpac-db",
    "dynamoTableName": "FsaFpacMetadata"
  },

  "ssm": {
    "landingBucketNameParam": "fpacFsaLandingBucketSSMName",
    "cleanBucketNameParam": "fpacFsaCleanBucketSSMName",
    "finalBucketNameParam": "fpacFsaFinalBucketSSMName",
    "glueJobRoleArnParam": "fpacFsaGlueJobRoleSSMArn",
    "etlRoleArnParam": "fpacFsaLambdaExecuteRoleARN",
    "thirdPartyLayerArnParam": "fpacfsaThirdPartyLayerArn",
    "customLayerArnParam": "fpacfsaCustomLayerArn"
  },
  "strparams": {
    "landingBucketNameParam": "punkdev-fpacfsa-landing-zone",
    "cleanBucketNameParam": "punkdev-fpacfsa-cleansed-zone",
    "finalBucketNameParam": "punkdev-fpacfsa-final-zone",
    "glueJobRoleArnParam": "arn:aws:iam::335965711887:role/FPACGlueCDKRole",
    "etlRoleArnParam": "arn:aws:iam::335965711887:role/FPACLambdaCDKRole",
    "thirdPartyLayerArnParam": "arn:aws:lambda:us-east-1:335965711887:layer:FSA-DEV-MDART-layer:1",
    "customLayerArnParam": "arn:aws:lambda:us-east-1:335965711887:layer:FSA-polars:1",
    "snsArnParam": "arn:aws:sns:us-east-1:335965711887:FSA-DEV-FMMI"
  },
  "paths": {
    "glueRootPath": "../src/glue/",
    "lambdaRootPath": "../src/lambda/"
  },

  "artifacts": {
    "artifactBucket": "dev-fmmi-artifacts",
    "prefix": "fmmi/"
  },

   "stepFunctions": {
    "createRoleIfMissing": false,
    "roleName": "FSA-dev-FPACCARS-SfnExec",
    "roleArn": "arn:aws:iam::335965711887:role/FSA-dev-FPACCARS-SfnExec"
  },
  "configUpload": {
    "functionName": "FSA-steam-dev-UploadConfig",
    "sourceDir": "fmmi",
    "inputBucket": "dev-cars-artifacts",
    "inputPrefix": "input/zips",
    "outputBucket": "punkdev-fpacfsa-final-zone",
    "outputPrefix": "fmmi",
    "minExpectedObjects": 2,
    "debug": true
  },
   "crawler": {
    "name": "CRWL-ODS",
    "databaseName": "fsa-fpac-fmmi-db",
    "description": "FMMI ODS crawler data sources (grouped by bucket)",
    "s3TargetsByBucket": [
      {
        "bucket": "punkdev-fpacfsa-final-zone",
        "prefixes": [
          "fmmi/fmmi_ods/CS_TBL_CMMT_ITEM/",
          "fmmi/fmmi_ods/CS_TBL_CUSTOMER/",
          "fmmi/fmmi_ods/CS_TBL_FUNC_AREA/",
          "fmmi/fmmi_ods/CS_TBL_FUND/",
          "fmmi/fmmi_ods/CS_TBL_FUNDED_PROGRAM/",
          "fmmi/fmmi_ods/CS_TBL_GL_ACCOUNT/",
          "fmmi/fmmi_ods/CS_TBL_WBS/",
          "fmmi/fmmi_ods/CS_TBL_VENDOR/",
          "fmmi/fmmi_ods/CS_TBL_PAYROLL/",
          "fmmi/fmmi_ods/CS_TBL_PO_HEADER/",
          "fmmi/fmmi_ods/CS_TBL_PO_ITEM/",
          "fmmi/fmmi_ods/CS_TBL_PURCHASING/",
          "fmmi/fmmi_ods/CS_TBL_INVOICE_DIS/",
          "fmmi/fmmi_ods/CS_TBL_COST_CENTER/",
          "fmmi/fmmi_ods/CS_TBL_MATERIAL_DOC/",
          "fmmi/fmmi_ods/CS_TBL_COMMITMENT/",
          "fmmi/fmmi_ods/CS_TBL_FUND_CENTER/",
          "fmmi/fmmi_ods/TMP_GL/",
          "fmmi/fmmi_ods/TMP_SA/",
          "fmmi/fmmi_ods/CS_TBL_GL/",
          "fmmi/fmmi_ods/CS_TBL_SYSTEM_ASSURANCE/",
          "fmmi/fmmi_ods/GL_SUMMARY/"
        ]
      }
    ]
  }
}