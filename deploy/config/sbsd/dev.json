    "customLayerArnParam": "arn:aws:lambda:us-east-1:241533156429:layer:FSA-polars:2"
  },
  "paths": {
    "glueRootPath": "../src/glue/",
    "lambdaRootPath": "../src/lambda/"
  },

  "artifacts": {
