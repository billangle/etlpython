  },
  "configUpload": {
    "functionName": "FSA-FPACDEV-UploadConfig",
    "sourceDir": "configs",
    "inputBucket": "fsa-dev-ops",
    "inputPrefix": "input/zips",
    "outputBucket": "c108-dev-fpacfsa-final-zone",
    "outputPrefix": "sbsd",
    "minExpectedObjects": 2,
    "debug": true
  },
   "GlueJobParameters": {
   "Connections" : [{
      "ConnectionName": "FSA-CERT-PG-DART114",
      "type": "JDBC",
      "VpcId": "vpc-05f9625eecaabbfd4",
      "SubnetIds": "subnet-02a134b722e1dd94c",
      "SecurityGroupIds": "sg-0d9f6f8633b91d354, sg-036039481aa5aeccc, sg-0906c3d7352c5889e, sg-00c4a92e918e642d4, sg-095cc861f7770b31a"
      }] ,
    "MaxConcurrency": "20",
